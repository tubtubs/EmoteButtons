							<AbsDimension x="1" y="-1" />
						</Offset>
					</Shadow>
				</FontString>
			</Layer>
		</Layers>
		<Frames>
			<Frame name="$parentLayer2">
				<Anchors>
					<Anchor point="LEFT">
						<Offset x="40" y="-50" />
					</Anchor>
					<Anchor point="TOP">
						<Offset x="50" y="-50" />
					</Anchor>
