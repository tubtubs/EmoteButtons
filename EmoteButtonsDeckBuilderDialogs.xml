					</Size>
					<Anchors>
						<Anchor point="TOP">
							<Offset>
								<AbsDimension x="0" y="5" />
							</Offset>
						</Anchor>
					</Anchors>
					<TexCoords left="0.2" right="0.8" top="0" bottom="0.6" />
				</Texture>
				<FontString name="$parentTitleString" font="GameFontNormal" text="Change Command">
					<!--Window Title -->
					<Size>
						<AbsDimension x="140" y="0" />
					</Size>
					<Anchors>
						<Anchor point="TOP">
							<Offset>
