					</TileSize>
					<EdgeSize>
						<AbsValue val="32" />
					</EdgeSize>
				</Backdrop>
			</Frame>
			<ScrollFrame name="$parent_ScrollFrame" inherits="UIPanelScrollFrameTemplate">
				<Size>
					<AbsDimension x="300" y="130"/>
				</Size>
				<Anchors>
					<Anchor point="LEFT">
						<Offset x="50" y="-60" />
					</Anchor>
					<Anchor point="TOP">
						<Offset x="50" y="-60" />
					</Anchor>
					<Anchor point="BOTTOM">
						<Offset x="50" y="60" />
					</Anchor>
				</Anchors>
				<ScrollChild>
					<EditBox name="$parent_CMDEditBox" multiLine="1" font="GameFontNormal">
						<Size>
							<AbsDimension x="300" y="480" />
						</Size>
						<Anchors>
							<Anchor point="TOPLEFT" relativeTo="$parent_ScrollFrame">
								<Offset x="-80" y="80" />
							</Anchor>
							<Anchor point="BOTTOMRIGHT"  relativeTo="$parent_ScrollFrame">
								<Offset x="20" y="20" />
							</Anchor>
						</Anchors>
						<Scripts>
							<OnShow>
								EmoteButtons_ChangeCMDFrame_OnShow()
							</OnShow>
							<OnTextChanged>
								local scrollBar = getglobal(this:GetParent():GetName().."ScrollBar")
								this:GetParent():UpdateScrollChildRect();
								local min;
								local max;
								min, max = scrollBar:GetMinMaxValues();
								if ( max > 0 and (this.max ~= max) ) then
									this.max = max;
									scrollBar:SetValue(max);
								end
								EmoteButtons_ChangeCMDFrame_SubmitButton_Update();
							</OnTextChanged>
							<OnEnterPressed>
							if (EmoteButtons_ChangeCMDFrame_SubmitButton:IsEnabled() ~= 0 ) then
								EmoteButtons_ChangeCMDFrame_SubmitButton_OnClick();
							end
							</OnEnterPressed>
							<OnEscapePressed>
							EmoteButtons_ChangeCMDFrame:Hide();
							</OnEscapePressed>
						</Scripts>
					</EditBox>
				</ScrollChild>
			</ScrollFrame>
			<Button name="$parent_SubmitButton" text="Submit" inherits="EmoteButtons_OptionButton_Template">
				<Size>
					<AbsDimension x="155" y="20" />
				</Size>
				<Anchors>
					<Anchor point="CENTER" relativeTo="$parent">
						<Offset>
							<AbsDimension x="0" y="-85"/>
						</Offset>
					</Anchor>
				</Anchors>
				<Scripts>
					<OnClick>
					EmoteButtons_ChangeCMDFrame_SubmitButton_OnClick()
                   </OnClick>
				</Scripts>
			</Button>
			<Button name="$parent_Close" inherits="UIPanelCloseButton">
				<Anchors>
					<Anchor point="TOPRIGHT" relativeTo="$parent" relativePoint="TOPRIGHT">
						<Offset>
							<AbsDimension x="-2" y="-4"/>
						</Offset>
					</Anchor>
				</Anchors>
				<Scripts>
					<OnClick>
						EmoteButtons_ChangeCMDFrame:Hide();
					</OnClick>
				</Scripts>
			</Button>
		</Frames>
		<Scripts>
			<OnLoad>             
				tinsert(UISpecialFrames, "EmoteButtons_ChangeCMDFrame");
			</OnLoad>
			<OnShow>

			</OnShow>
		</Scripts>
	</Frame>

